{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport CardRecomMixin from '../assets/js/CardRecom.js';\nimport CardDetail from './CardDetail.vue'; // CardDetail Ïª¥Ìè¨ÎÑåÌä∏ Î∂àÎü¨Ïò§Í∏∞\n\nexport default {\n  name: 'CardRecom',\n  component: {\n    CardDetail\n  },\n  // CardDetail Ïª¥Ìè¨ÎÑåÌä∏ Îì±Î°ù\n  mixins: [CardRecomMixin],\n  props: {\n    startPoint: String,\n    endPoint: String\n  },\n  data() {\n    return {\n      selectedCardId: null,\n      localStartPoint: this.startPoint,\n      // propsÎ°úÎ∂ÄÌÑ∞ Îç∞Ïù¥ÌÑ∞Î•º dataÎ°ú ÏÑ§Ï†ï\n      localEndPoint: this.endPoint,\n      // propsÎ°úÎ∂ÄÌÑ∞ Îç∞Ïù¥ÌÑ∞Î•º dataÎ°ú ÏÑ§Ï†ï\n      routes: JSON.parse(this.$route.query.routes || '[]') // queryÎ°úÎ∂ÄÌÑ∞ routes ÌååÏã±\n    };\n  },\n  methods: {\n    modalOpen(cardId) {\n      console.log(\"Opening modal for card:\", cardId);\n      this.selectedCardId = cardId;\n      this.$refs.modalWrap.classList.add('show');\n    },\n    modalClose() {\n      this.selectedCardId = null;\n      this.$refs.modalWrap.classList.remove('show');\n    },\n    filteredSubPaths(subPaths) {\n      return subPaths.filter(subPath => subPath.trafficType !== 3);\n    },\n    goToDetail(cardId) {\n      console.log(`Navigating to card detail for card id: ${cardId}`);\n      this.$router.push({\n        name: 'CardDetail',\n        params: {\n          id: cardId\n        }\n      });\n    }\n  },\n  watch: {\n    startPoint(newVal) {\n      this.localStartPoint = newVal;\n    },\n    endPoint(newVal) {\n      this.localEndPoint = newVal;\n    }\n  },\n  mounted() {\n    console.log('Received routes:', this.routes);\n  }\n};","map":{"version":3,"names":["CardRecomMixin","CardDetail","name","component","mixins","props","startPoint","String","endPoint","data","selectedCardId","localStartPoint","localEndPoint","routes","JSON","parse","$route","query","methods","modalOpen","cardId","console","log","$refs","modalWrap","classList","add","modalClose","remove","filteredSubPaths","su